{
  "name": "@readme/eslint-config",
  "version": "6.0.0",
  "description": "ReadMe coding standards",
  "main": "index.js",
  "author": "Jon Ursenbach <jon@ursenba.ch>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:readmeio/eslint-config.git"
  },
  "engines": {
    "node": "^12 || ^14",
    "npm": "^7"
  },
  "scripts": {
    "prepare": "husky install",
    "pretest": "npm run prettier",
    "prettier": "prettier --list-different --write \"./**/**.js\"",
    "test": "eslint .",
    "release": "conventional-changelog -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jest-dom": "^3.6.4",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "eslint-plugin-testing-library": "^4.1.2",
    "eslint-plugin-unicorn": "^34.0.1"
  },
  "peerDependencies": {
    "eslint": "^7.0.0",
    "prettier": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "conventional-changelog-cli": "^2.0.28",
    "husky": "^7.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
